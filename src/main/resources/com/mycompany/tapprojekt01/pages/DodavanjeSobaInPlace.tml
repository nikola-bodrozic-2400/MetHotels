<html t:type="layout" title="TapestryHibernateDemo Index"
      t:sidebarTitle="Framework Version"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
    <t:grid source="sobe" row="soba" exclude="id" t:inplace="true">
        <p:imeSobeCell>
            <div t:id="imeSobe" value="soba.imeSobe" t:type="jquery/inPlaceEditor" t:context="soba.id"/>
        </p:imeSobeCell>
        <p:spratCell>
            <div t:id="sprat" value="soba.sprat" t:type="jquery/inPlaceEditor"
                 t:context="soba.id"/>
        </p:spratCell> 
        <p:deleteCell>
            <t:actionlink t:id="delete" context="onesoba.id">Delete</t:actionlink>
        </p:deleteCell>       
    </t:grid>
</html>